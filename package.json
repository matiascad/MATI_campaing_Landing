    "build:smart": "nx run smartbuildlab-landing:build",
    "graph": "nx graph"
  },
  "devDependencies": {
    "@nx/react": "16.5.5",
    "@nx/vite": "16.5.5",
    "@nx/workspace": "16.5.5",
    "@types/node": "^22.15.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.45.0",
    "nx": "16.5.5",
    "path": "^0.12.7",
    "postcss": "^8.5.3",
    "typescript": "^5.0.4",
    "vite": "^4.4.5",
    "vitest": "^3.1.2"
  }
}
